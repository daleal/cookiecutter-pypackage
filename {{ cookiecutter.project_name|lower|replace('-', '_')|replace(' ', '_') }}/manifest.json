{
  "features": [
    {
      "name": "cli",
      "enabled": "{{ cookiecutter.include_cli == 'yes' }}",
      "resources": [
        "./{{ cookiecutter.project_name|lower|replace('-', '_')|replace(' ', '_') }}/cli"
      ]
    },
    {
      "name": "type_checking",
      "enabled": "{{ cookiecutter.include_type_checking == 'yes' }}",
      "resources": [
        "./mypy.ini"
      ]
    }
  ]
}
